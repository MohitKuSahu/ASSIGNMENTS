
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<html>
<head>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/ReportPage.css" />
</head>
<body>

    <div class="container">
        <center>
            <h1>Inventory Detailed Report</h1>
        </center>


        
        <div class="form-group col-md-4">
            <label for="exactDate">Select Exact Date:</label>
            <div class="input-group">
                <input type="date" id="exactDate" class="form-control" autocomplete="off">
            </div>
        </div>

        <div class="btn-container d-flex justify-content-between">
            <button id="btnSubmit" class="btn btn-primary">Check Transaction</button>
            <button class="btn btn-success" onclick="exportToPDF()">Export to PDF</button>
        </div>
    </div>

    <div id="report">
        <div id="list"></div>
    </div>


</body>
</html>

<script src="~/Scripts/Script.js"></script>
<script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.3.2/jspdf.debug.js"></script>
<script src="~/Scripts/Reports.js"></script>

