@model DemoUserManagement.Models.ContainerModel
<html>
<head>
</head>
<body>


    <h4>Add notes in this page</h4>

    <div class="row">
        <div class="col-md-4">
            <input type="text" id="NotesInput" class="form-control" />
        </div>
        <div class="col-md-4">
            <button id="NotesBtn" class="btn btn-success">Save Notes</button>
        </div>
        <input type="hidden" id="notesObjectID" value="@ViewBag.ObjectId" />
    </div>

    <div class="row">
        <div class="col-md-12">
            <div id="notesContainer">
                <table class="table table-bordered table-striped">
                    <thead class="thead-dark">
                        <tr>
                            <th>Notes ID</th>
                            <th>Note Text</th>
                            <th>Created Date</th>
                        </tr>
                     </thead>
                        @foreach (var note in ViewBag.Notes)
                        {
                            <tr>
                                <td>@note.NoteID</td>
                                <td>@note.NoteText</td>
                                <td>@note.CreatedDate</td>
                            </tr>

                        }
                </table>
            </div>
        </div>
    </div>
</body>
</html>
<script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
<script src="~/Scripts/Note.js"> </script>
